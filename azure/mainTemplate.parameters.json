{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "namePrefix": {
      "value": "{| vars.namePrefix |}"
    },
    "agentId": {
      "value": "{| vars.agentId |}"
    },
    "agentKey": {
      "value": "{| vars.agentKey |}"
    },
    "vmSize": {
      "value": "Standard_B2s"
    },
    "authenticationType": {
      "value": "password"
    },
    "adminUsername": {
      "value": "admin_service"
    },
    "adminPasswordOrKey": {
      "value": "{| vars.password |}"
    },
    "agentApiBaseUrl": {
      "value": "https://agent-api.refactr.it/v1"
    }
  }
}